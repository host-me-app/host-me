                android:text="@string/none"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/sp_date_desc_text"
                app:layout_constraintTop_toBottomOf="@+id/sp_title" />

            <TextView
                android:id="@+id/sp_download_text"
