<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
             android:layout_height="match_parent">


    <androidx.cardview.widget.CardView
        android:id="@+id/card_view1"
        android:layout_width="378dp"
        android:layout_height="176dp"
        android:layout_marginTop="50dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/rpconstraintLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/rpPickDateButton"
                android:layout_width="129dp"
                android:layout_height="50dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="104dp"
                android:text="@string/pick_date"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/rpDateText"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/rpImportButton"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="33dp"
                app:layout_constraintStart_toEndOf="@+id/rpImportText"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@android:drawable/ic_menu_upload"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/rptitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="232dp"
                android:text="@string/residence_permit"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/rpcheck"
                android:layout_width="24dp"
                android:layout_height="25dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="28dp"
                android:src="@android:drawable/presence_online"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/rptitle"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/rpImportText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/import_new_file"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rptitle" />

            <TextView
                android:id="@+id/rpDateDescText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="92dp"
                android:text="@string/expiration_date"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rptitle" />

            <TextView
                android:id="@+id/rpDateText"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="92dp"
                android:text="@string/none"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/rpDateDescText"
                app:layout_constraintTop_toBottomOf="@+id/rptitle" />

            <TextView
                android:id="@+id/rpDownloadText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="224dp"
                android:layout_marginTop="16dp"
                android:text="@string/download"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rptitle" />

            <ImageButton
                android:id="@+id/rpDownloadButton"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="33dp"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/rpDownloadText"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@android:drawable/stat_sys_download"
                tools:ignore="SpeakableTextPresentCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


